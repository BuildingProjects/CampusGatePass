{
  "expo": {
    "name": "InOut",
    "slug": "inout",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/inout-logo.jpg",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0E1A"
    },

    "ios": {
      "supportsTablet": true,
      "icon": "./assets/inout-logo.jpg"
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/inout-logo.jpg",
        "backgroundColor": "#0A0E1A"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "web": {
      "favicon": "./assets/inout-logo.jpg"
    },

    "plugins": ["expo-barcode-scanner"]
  }
}
